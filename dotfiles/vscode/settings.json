{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "editor.fontSize": 14,
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path.mac": "/Users/hng/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "vim.vimrc.enable": true,
"vim.leader": ",",
  "editor.fontFamily": "Fira Mono, Monaco, 'Courier New', monospace",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "vim.statusBarColorControl": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["gitlens.toggleFileBlame"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["gitlens.diffWithPrevious"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["editor.action.organizeImports"]
    }
],
"workbench.iconTheme": "material-icon-theme",
"gitlens.blame.avatars": false,
"gitlens.blame.format": "${author|20?} ${agoOrDate|14-}",
"jest.autoEnable": false,
"workbench.colorTheme": "Default Light+"
}
