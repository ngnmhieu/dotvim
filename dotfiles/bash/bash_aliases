alias ls='ls -GF' # ls with color
alias l='ls -GF' 
alias ll='ls -lGhF'
alias la='ls -laGhF'
alias x='exit'
alias v='vim'
alias ..='cd ..'

alias pj='cd ~/Projects'
alias dropbox='cd ~/Dropbox'
alias dotvim='cd ~/.vim/'

alias gs='git status'
alias gc='git commit'
alias ga='git add'
alias gb='git branch'

alias bashrc='vim ~/.bashrc'
alias so='source'
alias tmux="TERM=screen-256color-bce tmux"
